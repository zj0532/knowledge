        <Action ActionType="Judge">
            <Condition Type="Approve">1</Condition>
        </Action>
    </Step>
    <Step StepNumber="9" Description="审批不通过" ParentNumber="7">
        <Action ActionType="Judge">
            <Condition Type="Approve">0</Condition>
        </Action>
    </Step>

